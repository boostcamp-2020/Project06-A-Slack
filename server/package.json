{
  "name": "slack-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env MODE=dev nodemon --exec ts-node ./src/app.ts",
    "dev2": "cross-env MODE=dev tsc-watch --onSuccess \" node dist/app.js\" ",
    "start": "pm2 start ./dist/app.js"
  },
  "_moduleAliases": {
    "@": "./dist",
    "@config": "./dist/config",
    "@lib": "./dist/lib",
    "@middlewares": "./dist/middlewares",
    "@models": "./dist/models",
    "@routes": "./dist/routes",
    "@services": "./dist/services",
    "@controllers": "./dist/controllers",
  },
  "dependencies": {
    "@types/express-mysql-session": "^2.1.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mysql-session": "^2.1.4",
    "express-session": "^1.17.1",
    "http-errors": "^1.8.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.2",
    "@types/http-errors": "^1.8.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.0.5"
  }
}
